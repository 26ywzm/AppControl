            "icon": "cmd.png"
        },
        "taskmgr": {
            "name": "任务管理器",
            "start": "taskmgr.exe",
            "kill": "taskkill /F /IM taskmgr.exe",
            "description": "Windows 任务管理器 - 监控系统性能和进程",
            "icon": "taskmgr.png"
        },
        "control": {
            "name": "控制面板",
            "start": "control.exe",
            "kill": "taskkill /F /IM control.exe",
            "description": "Windows 控制面板 - 系统设置和配置",
            "icon": "control.png"
        }
