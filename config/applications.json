{
    "applications": {
        "notepad": {
            "name": "记事本",
            "start": "notepad.exe",
            "kill": "taskkill /F /IM notepad.exe",
            "description": "Windows 记事本 - 用于快速编辑文本文件",
            "icon": "notepad.png"
        },
        "calculator": {
            "name": "计算器",
            "start": "calc.exe",
            "kill": "taskkill /F /IM Calculator.exe /IM ApplicationFrameHost.exe",
            "description": "Windows 计算器 - 执行基本和科学计算",
            "icon": "calculator.png"
        },
        "paint": {
            "name": "画图",
            "start": "mspaint.exe",
            "kill": "taskkill /F /IM mspaint.exe",
            "description": "Windows 画图 - 简单的图像编辑工具",
            "icon": "paint.png"
        },
        "explorer": {
            "name": "文件资源管理器",
            "start": "explorer.exe",
            "kill": "taskkill /F /IM explorer.exe",
            "description": "Windows 文件资源管理器 - 浏览和管理文件",
            "icon": "explorer.png"
        },
        "wordpad": {
            "name": "写字板",
            "start": "write.exe",
            "kill": "taskkill /F /IM wordpad.exe",
            "description": "Windows 写字板 - 支持格式的文本编辑器",
            "icon": "wordpad.png"
        },
        "cmd": {
            "name": "命令提示符",
            "start": "cmd.exe",
